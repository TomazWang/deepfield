{
  "name": "deepfield",
  "version": "1.0.0",
  "description": "CLI tool for AI-driven knowledge base building",
  "main": "dist/cli.js",
  "bin": {
    "deepfield": "./dist/cli.js",
    "df": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup src/cli.ts --format cjs --dts --clean --sourcemap && npm run copy:templates",
    "copy:templates": "mkdir -p dist/templates && cp -r templates/* dist/templates/",
    "dev": "tsx src/cli.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "knowledge-base",
    "brownfield",
    "documentation",
    "cli"
  ],
  "author": "Deepfield Contributors",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "zod": "^3.22.4",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/inquirer": "^9.0.7",
    "@types/fs-extra": "^11.0.4",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
